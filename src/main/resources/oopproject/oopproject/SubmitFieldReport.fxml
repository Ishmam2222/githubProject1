<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="oopproject.oopproject.SubmitFieldReport">
    <children>
        <Label layoutX="20.0" layoutY="20.0" style="-fx-font-size: 18px;" text="Submit Field Report" />
        <Label layoutX="20.0" layoutY="60.0" text="Verification Summary:" />
        <TextArea fx:id="summaryField" layoutX="20.0" layoutY="80.0" prefHeight="100.0" prefWidth="560.0" />
        <Label layoutX="20.0" layoutY="200.0" text="Attach any (Photos/Documents)as required :" />
        <Button layoutX="20.0" layoutY="300.0" onAction="#submitReport" text="Submit Report" />
        <Button layoutX="120.0" layoutY="300.0" onAction="#backToDashboard" text="Back to Dashboard" />
    </children>
</AnchorPane>
