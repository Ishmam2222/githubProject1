<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="oopproject.oopproject.ConfirmingDetailsSubmittingUNID">
    <Label layoutX="20.0" layoutY="70.0" style="-fx-font-size: 18px;" text="Summary of Application Details:" />
    <TableView fx:id="citizenTable" layoutX="20.0" layoutY="100.0" prefHeight="200.0" prefWidth="760.0">
        <columns>
            <TableColumn fx:id="nameColumn" prefWidth="200.0" text="Name" />
            <TableColumn fx:id="dobColumn" prefWidth="200.0" text="Date of Birth" />
            <TableColumn fx:id="addressColumn" prefWidth="360.0" text="Address" />
        </columns>
    </TableView>
    <Button layoutX="580.0" layoutY="310.0" onMouseClicked="#loadTableFromFile" text="Proceed to Summary" />
    <Button layoutX="20.0" layoutY="526.0" onAction="#logOut" text="Log Out" />
    <Button layoutX="100.0" layoutY="526.0" onMouseClicked="#backToDashboard" text="Back to Dashboard" />
</AnchorPane>
